<script type="text/javascript">
var more_status = 0;
jQuery(function($){
	$('#J_ItemList').more({'address': 'pintuan.php?act=asynclist', 'spinner_code':'<div style="text-align:center; margin:10px;"><img src="{$ectouch_themes}/images/loader.gif" /></div>'})
	$(window).scroll(function () {
		if (more_status == 0 && $(window).scrollTop() == $(document).height() - $(window).height()) {
			$('.get_more').click();
			more_status = 1;
			setTimeout(function(){
				more_status = 0;
			}, '1000');
		}
	});
});
</script>
