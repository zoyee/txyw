{$stats_code}
</head>
<body>
<div id="page">
<div id="tbh5v0">
<div class="swiper-container swiper-container-horizontal">
  <!-- 分类 -->
  <div class="swiper-wrapper">
  	 <div class="swiper-slide" onclick="location.href='suppliers.php?act=agent_page'">我的代理</div>
  	 <div class="swiper-slide" onclick="location.href='suppliers.php?act=not_agent_page'">邀请代理</div>
  	 <div class="swiper-slide active-nav" onclick="location.href='suppliers.php?act=f2f_agent_page'">面对面邀请</div>
  </div>
</div>

  <div class="content"> 
  	{if user_qr}
    <div style="margin-top: 100px; text-align:center; font-size: 14px;">用户扫一扫下面的二维码，即可成为您的认证代理商</div> 
    <div style="margin-top: 20px; text-align:center;"><img style="width: 50%;" src="{$user_qr}"/></div>
    {else}
    <div style="margin-top: 100px; text-align:center; font-size: 14px;">网络不给力，请刷新试试！</div>
    {/if}
	<script type="text/javascript" src="{$ectouch_themes}/js/jquery.more.js"></script>
    <script type="text/javascript">
    jQuery(function($){
    	var mySwiper = new Swiper('.swiper-container',{
   		 direction : "horizontal",
   		 watchSlidesProgress : true,
   		 watchSlidesVisibility : true,
   		 slidesPerView : 3,
   		 onInit: function(swiper){
   		 },
   		 onTap: function(){
   		 }
   	 });
    	
    	$(window).scroll(function(){
    	    var mytop = $(window).scrollTop();
    	    if( mytop > $(".swiper-container").offset().top){
    	    	if(!$(".swiper-container").hasClass("float-top")){
    	        	$(".swiper-container").addClass("float-top")
    	    	}
    	    }else if(mytop == 0){
    	    	$(".swiper-container").removeClass("float-top")
    	    }

    	});
    });
    </script>
    <!--#订单列表界面 end--> 


</div>
</div>
 </div>
