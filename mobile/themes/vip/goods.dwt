<script type="text/javascript">
//获取供应商的联系方式
function get_contact(suppliers_id){
	jQuery.get('suppliers.php?act=get_contact&id=' + suppliers_id, function(data){
		if(data.errcode > 0){
			openAlertDiv(data.errmsg);
		}else{
			openAlertDiv("供应商联系方式已发送，请查看公众号消息!");
		}
	}, "json");
}
 
var process_request = "{$lang.process_request}";

function haibao(goods_id){
	openAlertDiv("正在生成海报，稍后请查看公众号消息!");
	jQuery.get('haibao.php?act=creat_by_goods&goods_id=' + goods_id, {}, function(data){
		if(data.errcode > 0){
			openAlertDiv(data.errmsg);
		}else{
			openAlertDiv("海报已生成，请查看公众号消息!");
		}
	}, "json");
}

//申请代理认证
function apply_agent(suppliers_id){
	jQuery.get('suppliers.php?act=apply_agent&suppliers_id=' + suppliers_id, function(data){
		if(data.errcode > 0){
			openAlertDiv(data.message);
		}else{
			openAlertDiv(data.message);
		}
	}, "json");
}

function need_agent(){
	openAlertDiv("必须先申请成为认证代理才能购买！");
}
</script>
