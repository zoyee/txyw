<script type="text/javascript">

$(function() {
    $('.swiper-free').each(function(){
    	new Swiper(this, {
    		freeMode : true,
    		freeModeFluid: true,
    		slidesPerView : 4
    	});
    	/* $(this).swiper({
    		freeMode : true,
    		freeModeFluid: true,
    		slidesPerSlide : 4,
    		updateOmImagesReady: true,
    		onImageReady: function(){
    			alert(123);
    		}
    	}) */;
    });

    $('#search_text').click(function(){
        $(".showpage").children('div').hide();
        $("#search_hide").css('position','fixed').css('top','0px').css('width','100%').css('z-index','999').show();
        $("#keyword").focus();
    })
    $('#get_search_box').click(function(){
        $(".showpage").children('div').hide();
        $("#search_hide").css('position','fixed').css('top','0px').css('width','100%').css('z-index','999').show();
    })
    $("#search_hide .close").click(function(){
        $(".showpage").children('div').show();
        $("#search_hide").hide();
    })
});
</script>

<script>
$('.search-type li').click(function() {
    $(this).addClass('cur').siblings().removeClass('cur');
    $('#searchtype').val($(this).attr('num'));
});
$('#searchtype').val($(this).attr('0'));
</script>

<script src="http://res.wx.qq.com/open/js/jweixin-1.0.0.js"></script>
<script>
<!-- {/literal} -->
  wx.config({
    debug: false,
    appId: '{$signPackage.appId}',
    timestamp: {$signPackage.timestamp},
    nonceStr: '{$signPackage.nonceStr}',
    signature: '{$signPackage.signature}',
    jsApiList: [
        'onMenuShareTimeline',
        'onMenuShareAppMessage' 
    ]
  });
 wx.ready(function () {
	//青山老农监听“分享给朋友”
    wx.onMenuShareAppMessage({
      title: '{$page_title}',
      desc: '{$page_title}',
      link: '{$url}',
      imgUrl: '{$w_picurl}',
      trigger: function (res) {
		
		{if $url}
        alert('恭喜！分享可以获取提成哦！');
		{else}
		alert('糟糕，需要分销商登录才能获得提成哦！');
		{/if}
		
      },
      success: function (res) {
		{if $dourl }
        window.location.href="{$dourl}&type=1"; 
		{/if}
      },
      cancel: function (res) {
        alert('很遗憾，您已取消分享');
      },
      fail: function (res) {
        alert(JSON.stringify(res));
      }
    });

	//分享到朋友圈青山老农
    wx.onMenuShareTimeline({
      title: '{$page_title}',
      link: '{$url}',
      imgUrl: '{$w_picurl}',
      trigger: function (res) {
			
        {if $url}
			alert('恭喜！分享可以获取提成哦！');
		{else}
			alert('糟糕，需要分销商登录才能获得提成哦！');
		{/if}
      },
      success: function (res) {
       	{if $dourl }
        window.location.href="{$dourl}&type=2"; 
		{/if}
      },
      cancel: function (res) {
         alert('很遗憾，您已取消分享');
      },
      fail: function (res) {
        alert(JSON.stringify(res));
      }
    });


});
<!-- {literal} -->
</script>
